rces":["MultiVideo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"MultiVideo.vue","sourceRoot":"src/views","sourcesContent":["<template>\n<div class=\"card-wrapper\">\n<div class=\"row\">\n    <div class=\"col-md-12\">\n        <div class=\"col-md-6\">\n            <form @submit.prevent=\"handleSubmit()\">\n                <div class=\"col-md-8\">\n                    <input v-model=\"searchText\"  class=\"form-control\" type=\"text\" placeholder=\"Search here\">\n                </div>\n                <div class=\"col-md-4\">\n                    <button class=\"btn btn-primary\">Search</button> \n                </div>\n            </form>\n        </div>\n        <div class=\"col-md-6\">\n            <router-link :to=\"'/program/singleVideo'\"><button class=\"btn btn-success\">Upload new program</button></router-link>\n        </div>\n    </div>\n</div>\n<div v-for=\"program in multiVideos.content\" :key=\"program.id\"  class=\"card\">\n    <div class=\"row \">\n        <div class=\"col-md-4\">\n            <img width=\"300\" height=\"180\"  :src=\"program.imgUrls.thumbnail\" class=\"w-100\">\n        </div>\n        <div class=\"col-md-8 px-3\">\n            <div class=\"card-block col-md-5 px-3\">\n                <router-link :to=\"'/seasons/'+program.id\"><h3 class=\"card-title\">{{ program.name }}</h3></router-link>\n                <p class=\"card-text\"><b>Upload Id : </b>xxxx</p>\n                <p class=\"card-text\"><b>Start Date : </b>{{ changeTime(program.startDate) }}</p>\n                <p class=\"card-text\"><b>Expiry Date : </b>{{ changeTime(program.expiryDate) }}</p>\n                <p class=\"card-text\"><b>Upload Date : </b>{{ changeTime(program.creationDate) }}</p>\n            </div>\n            <div class=\"card-block col-md-4 px-3\">\n                <div class=\"help\"> </div>\n                <p class=\"card-text\"><b>Description : </b>{{ program.description }}</p>\n                <p class=\"card-text\"><b>Category : </b>{{ program.category.name }}</p>\n                <p class=\"card-text\"><b>Languages : </b>{{ program.languages }}</p>\n                <p class=\"card-text\"><b>Parental Rating : </b>{{ program.parentalRating }}</p>\n            </div>\n            <div class=\"card-block col-md-3 px-3\">\n                <div class=\"help1\"> </div>\n                <p><button @click=\"deleteProgram(program.id)\"  class=\"btn btn-danger\">Remove</button></p>\n                <router-link :to=\"'/seasonVideoForm'\"><button class=\"btn btn-primary\">Edit</button></router-link>\n            </div>\n        </div>\n    </div>\n</div>\n<div class=\"col-md-12\">\n<div v-if=\"multiVideos.content!=0\"  class=\"col-md-12\">\n    <div class=\"clearfix\">\n        <div class=\"hint-text\">Showing <b><span v-if=\"multiVideos.number!=(multiVideos.totalPages-1)\">{{((page.pageNumber+1)*multiVideos.numberOfElements)}}</span>\n            <span v-else>{{((multiVideos.number*multiVideos.size)+multiVideos.numberOfElements)}}</span></b> out of <b>{{ multiVideos.totalElements }}</b> entries</div>\n        <!-- <div class=\"hint-text\">Showing <b>{{ allAudits.size }}</b> out of <b>{{ allAudits.totalElements }}</b> entries</div> -->\n        <ul class=\"pagination\">\n            <button v-if=\"page.pageNumber!=0\" @click=\"prvPage()\"   class=\"btn btn-primary\">Prev</button>\n            <span> {{page.pageNumber+1}} Out of {{multiVideos.totalPages}}</span>\n            <button v-if=\"page.pageNumber!=(multiVideos.totalPages-1)\" @click=\"nextPage()\" style=\"margin:10px;\" class=\"btn btn-primary\">Next</button>\n        </ul>\n    </div>\n</div> \n</div>      \n</div>\n</template>\n\n<script src=\"./js/multiVideo.js\"></script>\n\n\n<style>\n.card-wrapper {\n    padding:10px;\n}\n.card {\n    border:1px solid black;\n    margin:10px;\n}\n.pagination {\n        float: right;\n        margin: 0 0 5px;\n    }\n    .pagination li a {\n        border: none;\n        font-size: 95%;\n        width: 30px;\n        height: 30px;\n        color: #999;\n        margin: 0 2px;\n        line-height: 30px;\n        border-radius: 30px !important;\n        text-align: center;\n        padding: 0;\n    }\n    .pagination li a:hover {\n        color: #666;\n    }   \n    .pagination li.active a {\n        background: #03A9F4;\n    }\n    .pagination li.active a:hover {        \n        background: #0397d6;\n    }\n    .pagination li.disabled i {\n        color: #ccc;\n    }\n    .pagination li i {\n        font-size: 16px;\n        padding-top: 6px\n    }\n    .hint-text {\n        float: left;\n        margin-top: 6px;\n        font-size: 95%;\n    }\n</style>\n"]}]}