           },\n            tag:'',\n            tags: [],\n            isEdit:true\n        }\n    },\n    computed: {\n        ...mapGetters([\n            'languages'\n        ])\n    },\n    methods: {\n        ...mapActions([\n            'addLanguage',\n            'getLanguages',\n            'deleteLanguageByName',\n            'editLanguage'\n        ]),\n        handleSubmit() {\n            this.addLanguage(this.language).then ( (resp) => {\n                if(resp) {\n                    this.$swal(this.language[0].name+' Added')\n                    this.getLanguages()\n                    this.language[0].name=\"\"\n                } else {\n                    this.$swal(this.language[0].name+\" already exists\")\n                    this.tags=[]\n                }\n            }).catch((err) => {\n                console.log(err)\n            })\n        },\n        init() {\n            this.getLanguages()\n        },\n        handleDelete(name) {\n            this.deleteLanguageByName(name).then( (resp) => {\n                console.log(resp)\n                if(resp) {\n                    this.$swal(name.name+\" Deleted\")\n                    this.getLanguages()\n                }\n            }).catch( (err) => {\n                console.log(err)\n            })\n        },\n        handleEdit(language) {\n            this.language[0].name = language.name           \n        }\n    },\n    mounted() {\n        this.init()\n    }\n    \n}\n</script>\n\n\n<style>\n.box {\n    padding:20px;\n    margin:20px;\n    border-radius: 10px;\n\tbox-shadow: 0px 8px 20px 0px rgba(0, 0, 0, 0.15);\n}\n.form-heading {\n    text-align:center;\n    color: #2980B9;\n}\n</style>\n"]}]}